{
  "name": "jfalkenhagen/blizzard-api-service",
  "description": "Access wrapper for the Blizzard BattleNet API",
  "url": "https://gitlab.jfdnk.de/open-source/blizzard-api-service/",
  "type": "library",
  "license": "MIT",
  "version": 0.1,
  "authors": [
    {
      "name": "Jared Falkenhagen",
      "email": "jared@falkenhagen.de"
    }
  ],
  "autoload": {
    "psr-4" : {
      "BlizzardApiService\\" : "src"
    }
  },
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.0",
    "league/oauth2-client": "^2.3"
  },
  "autoload-dev": {
    "psr-4": {
      "BlizzardApiService\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.3"
  }
}